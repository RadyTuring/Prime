@model TeacherCodeAdminDtoCsv
@{
    string title = "Import Teacher Code(s)";
    ViewData["Title"] = title;
    <partial name="_titleBar" model="@title" />
}

<div class="row">
    <div class="col-md-4">
        <form asp-action="Import" enctype="multipart/form-data">
            <div asp-validation-summary="All" class="text-danger"></div>
            <div class="form-group">
                <label for="Students" class="col-sm-2 control-label">Students CSV:</label>
                <div class="col-sm-10">

                    <input type="file" asp-for="TeacherCodeFile" class="form-control" placeholder="Teacher Codes CSV" />
                    <span asp-validation-for="TeacherCodeFile" class="text-danger"></span>
                </div>
            </div>
            <br />
            <div class="form-group">
                <input type="submit" value="Import" class="btn btn-primary btn-block" />
            </div>
        </form>
    </div>
</div>
<br />
<div>
    <a asp-controller="Filter" asp-action="TeacherCodes">Back to List</a>
</div>

@section Scripts {
    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
}
