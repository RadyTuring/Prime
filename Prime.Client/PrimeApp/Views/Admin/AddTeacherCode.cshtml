@model Dto.TeacherCodeAdminDto
@{
    string title = "Add Teacher Code";
    ViewData["Title"] = title;
    <partial name="_titleBar" model="@title" />
   
}
<div class="row">
    <div class="col-md-4">
        <form asp-action="AddTeacherCode">
            <div asp-validation-summary="All" class="text-danger"></div>
            <div class="form-group">
                <label asp-for="TeacherCode" class="control-label"></label>
                <input asp-for="TeacherCode" class="form-control" />
                <span asp-validation-for="TeacherCode" class="text-danger"></span>
            </div>
            <br />
            <div class="form-group">
                <input type="submit" value="Add" class="btn btn-primary btn-block" />
            </div>
        </form>
    </div>
</div>
<br />
<div>
    <a asp-controller="Filter" asp-action="TeacherCodes">Back to List</a>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}
